package cliente;

public class ConversacionPrivada {
	ChatClient sender;
	ChatClient receiver;
	public ConversacionPrivada(ChatClient sender, ChatClient receiver) {
		super();
		this.sender = sender;
		this.receiver = receiver;
	}
	public ChatClient getSender() {
		return sender;
	}
	public void setSender(ChatClient sender) {
		this.sender = sender;
	}
	public ChatClient getReceiver() {
		return receiver;
	}
	public void setReceiver(ChatClient receiver) {
		this.receiver = receiver;
	}
	
	
}
